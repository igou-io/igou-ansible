terminationGracePeriodSeconds: 30
domain:
  resources:
    requests:
    memory: 1024M
  devices:
    disks:
    - name: containerdisk
        disk:
        bus: virtio
    - name: emptydisk
          disk:
          bus: virtio
    - disk:
          bus: virtio
          name: cloudinitdisk
  volumes:
  - name: containerdisk
    containerDisk:
    image: kubevirt/fedora-cloud-container-disk-demo:latest
  - name: emptydisk
    emptyDisk:
    capacity: "22Gi"
